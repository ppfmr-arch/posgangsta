    "$envmap" "env_cubemap"
    "$envmaptint" "[0 0 0]"
    "$envmapfresnel" "0"


    "$bumpMap"                "bitminers2/bm1_wires_normal"
    //"$normalmapalphaenvmapmask" "1"

    "$surfaceprop" "Metal"
